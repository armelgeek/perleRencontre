{% extends 'base.html.twig' %}

{% block title %}Hello AbonnementController!
{% endblock %}

{% block body %}
	<style>
		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper code {
			background: #F5F5F5;
			padding: 2px 6px;
		}
	</style>

	<div class="example-wrapper">
		<ul>
			{% for ab in abonnement %}
				<li>{{ ab.name}}({{ab.perle}}
					perles/{{ab.price}}
					euros)</li>
			{% endfor %}
		</ul>

	</div>
{% endblock %}
